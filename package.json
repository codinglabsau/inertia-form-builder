{
  "name": "@codinglabsau/inertia-form-builder",
  "version": "1.4.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/inertia-form-builder.umd.cjs",
  "module": "./dist/inertia-form-builder.js",
  "exports": {
    ".": {
      "import": "./dist/inertia-form-builder.js",
      "require": "./dist/inertia-form-builder.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p format lint type-check && vite build",
    "type-check": "vue-tsc --declaration --emitDeclarationOnly",
    "format": "prettier \"src/**/*.{js,ts,vue}\" --write",
    "lint": "eslint \"src/**/*.{js,ts,vue}\" --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "publishConfig": {
    "@codinglabsau:registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@codinglabsau/ui": "^2.0.3",
    "@inertiajs/vue3": "^1.0.2",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^3.0.7",
    "vitepress": "^1.0.0-rc.10",
    "vue": "^3.2.37",
    "vue-tsc": "^1.2.0"
  }
}
