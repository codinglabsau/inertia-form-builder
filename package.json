{
  "name": "@codinglabsau/inertia-form-builder",
  "version": "2.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/inertia-form-builder.umd.cjs",
  "module": "./dist/inertia-form-builder.js",
  "exports": {
    ".": {
      "import": "./dist/inertia-form-builder.js",
      "require": "./dist/inertia-form-builder.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p format lint type-check && vite build",
    "type-check": "vue-tsc --declaration --emitDeclarationOnly",
    "format": "prettier \"app/**/*.{js,ts,vue}\" \"src/**/*.{js,ts,vue}\" --write",
    "lint": "eslint \"app/**/*.{js,ts,vue}\" \"src/**/*.{js,ts,vue}\" --fix",
    "docs:build": "vite build --config vite.docs.config.ts",
    "docs:preview": "vite preview --outDir docs-dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test"
  },
  "publishConfig": {
    "@codinglabsau:registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@codinglabsau/gooey": "^2.0.0",
    "@inertiajs/vue3": "^2.2.0",
    "laravel-precognition-vue-inertia": "^0.7.2",
    "tailwindcss": "^3.3.0 || ^4.0.0",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "laravel-precognition-vue-inertia": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "devDependencies": {
    "@codinglabsau/gooey": "^2.0.0",
    "@eslint/js": "^9.39.0",
    "@inertiajs/vue3": "^2.2.0",
    "@playwright/test": "^1.49.0",
    "@shikijs/markdown-it": "^3.22.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^14.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^26.1.0",
    "laravel-precognition-vue-inertia": "^0.7.2",
    "lucide-vue-next": "^0.562.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "shiki": "^3.22.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "unplugin-vue-markdown": "^29.2.0",
    "vite": "^7.3.1",
    "vitest": "^3.2.0",
    "vue": "^3.5.16",
    "vue-router": "^4.4.4",
    "vue-tsc": "^3.2.2"
  }
}
